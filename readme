# Corrigir automaticamente os erros do ESLint em arquivos .js dentro da pasta src
yarn eslint --fix src --ext .js

# Inicializar um novo projeto com Yarn
yarn init 

# Instalar as dependências do projeto
yarn

# Iniciar a aplicação
yarn start


SELECT * FROM cust WHERE email = "chilengue@gmail.com";

npx sequelize db:migrate:undo


npx sequelize db:migrate:undo:all  # Reverter todas as migrations
npx sequelize db:migrate           # Rodar novamente as migrations
